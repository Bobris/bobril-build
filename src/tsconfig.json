{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts",
        "../typings/node/**/*.d.ts",
        "../typings/bluebird/**/*.d.ts",
        "../typings/png-async/**/*.d.ts",
        "!./test/*.ts",
        "../node_modules/typescript/bin/typescript.d.ts"
    ],
    "files": [
        "./bobrilDepsHelpers.ts",
        "./buildHelpers.ts",
        "./compilationCache.ts",
        "./evalNode.ts",
        "./imageOps.ts",
        "./imgCache.ts",
        "./spriter.ts",
        "./translationCache.ts",
        "../typings/node/node.d.ts",
        "../typings/bluebird/bluebird.d.ts",
        "../typings/png-async/png-async.d.ts",
        "../node_modules/typescript/bin/typescript.d.ts"
    ]
}
