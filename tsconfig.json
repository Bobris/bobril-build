{
    "version": "1.6.2",
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false
    },
    "compileOnSave": false,
    "filesGlob": [
        "./cli.ts",
        "./index.ts",
        "./src/*.ts",
        "./spec/*.ts",
        "./node_modules/typescript/lib/typescript.d.ts",
        "./typings/uglifyjs/**/*.d.ts",
        "./typings/chokidar/**/*.d.ts",
        "./typings/node/**/*.d.ts",
        "./typings/png-async/**/*.d.ts",
        "./typings/jasmine/jasmine.d.ts"
    ],
    "files": [
        "./cli.ts",
        "./index.ts",
        "./src/bobrilDepsHelpers.ts",
        "./src/buildHelpers.ts",
        "./src/compilationCache.ts",
        "./src/debounce.ts",
        "./src/evalNode.ts",
        "./src/imageOps.ts",
        "./src/imgCache.ts",
        "./src/pathUtils.ts",
        "./src/spriter.ts",
        "./src/bundler.ts",
        "./src/translationCache.ts",
        "./spec/compilationCacheSpec.ts",
        "./spec/evalNodeSpec.ts",
        "./spec/bundlerSpec.ts",
        "./spec/imageOpsSpec.ts",
        "./spec/spriterSpec.ts",
        "./node_modules/typescript/lib/typescript.d.ts",
        "./typings/chokidar/chokidar.d.ts",
        "./typings/uglifyjs/uglifyjs.d.ts",
        "./typings/node/node.d.ts",
        "./typings/png-async/png-async.d.ts",
        "./typings/jasmine/jasmine.d.ts"
    ]
}
